<view class='container' bindlongtap='saveImage'>
	<view class="avatar_box">
		<image class="avatar" wx:if='{{avatarUrl.length}}'></image>
		<open-data class='avatar' type="userAvatarUrl" v-else></open-data>
	</view>
	<view class="content">
		<image wx:if='{{count==0}}' class="reard_image" src="https://reportzhej.hualife.cc:8443/zjhx-reports/views/images/yinbao/4/yixing.png"></image>
		<image wx:if='{{count==1}}' class="reard_image" src="https://reportzhej.hualife.cc:8443/zjhx-reports/views/images/yinbao/4/erxing.png"></image>
		<image wx:if='{{count==2}}' class="reard_image" src="https://reportzhej.hualife.cc:8443/zjhx-reports/views/images/yinbao/4/sanxing.png"></image>
		<image wx:if='{{count==3}}' class="reard_image" src="https://reportzhej.hualife.cc:8443/zjhx-reports/views/images/yinbao/4/sixing.png"></image>
		<image wx:if='{{count==4}}' class="reard_image" src="https://reportzhej.hualife.cc:8443/zjhx-reports/views/images/yinbao/4/wuxing.png"></image>
	</view>
</view>